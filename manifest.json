{
  "manifest_version": 2,
  "name": "Debug Header Injector",
  "version": "1.0",
  "description": "Inject custom headers to matching domains",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>",
    "storage"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Debug Header Injector"
  },
  "icons": {
    "48": "icon.png"
  }
}
